@{
    Layout = null;
    @model List<Kodcamp.Models.LogReport.Contact>;
    var contacts = Model;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>Mesajlar</title>
    <link href="/profile/css/styles.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/js/all.min.js" crossorigin="anonymous"></script>
</head>
<body class="sb-nav-fixed">
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <!-- Navbar Brand-->
        <a class="navbar-brand ps-3" href="/">Siteye Dön</a>
        <!-- Sidebar Toggle-->
        <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars"></i></button>
        <!-- Navbar Search-->
        <form class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0">
            <div class="input-group">
                <input class="form-control" type="text" placeholder="Ara..." aria-label="Search for..." aria-describedby="btnNavbarSearch" />
                <button class="btn btn-primary" id="btnNavbarSearch" type="button"><i class="fas fa-search"></i></button>
            </div>
        </form>
        <!-- Navbar-->
        <ul class="navbar-nav ms-auto ms-md-0 me-3 me-lg-4">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
                    <li><a class="dropdown-item" href="/profile/logout">Logout</a></li>
                </ul>
            </li>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading">Profil</div>
                        <a class="nav-link" href="/mng/sys/kodcamptr">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            Anasayfa
                        </a>
                        <a class="nav-link" href="/management/category">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            Kategoriler
                        </a>
                        <a class="nav-link" href="/management/news">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            Haberler
                        </a>
                        <a class="nav-link" href="/management/logs">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            Raporlar
                        </a>
                        <a class="nav-link" href="/Management/Contact">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            İletişim Talepleri
                        </a>

                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <div class="small">aaa </div>
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-4">
                    <h2 class="mt-4">Mesajlarım</h2>
                    <ol class="breadcrumb mb-4">
                        <li class="breadcrumb-item"><a href="/profile">Profil Yönetimi</a></li>
                        <li class="breadcrumb-item active">Mesajlar</li>
                    </ol>

                    <div class="card mb-4">
                        <div class="card-header">
                            <i class="fas fa-table me-1"></i>
                            Genel Bakış
                        </div>
                        <div class="card-body">
                            <table id="datatablesSimple">
                                <thead>
                                    <tr>
                                        <th>Başlık</th>
                                        <th>Detay</th>
                                        <th>Mail</th>
                                        <th>İsim</th>
                                        <th>Tarih</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    @{
                                        for (int i = 0; i < contacts.Count; i++)
                                        {
                                            <tr>
                                                <th>@contacts[i].Title</th>
                                                <th>@contacts[i].Detail</th>
                                                <th>@contacts[i].Email</th>
                                                <th>@contacts[i].Name</th>
                                                <th>@contacts[i].DateCreated</th>
                                            </tr>
                                        }
                                    }

                                </tfoot>
                                <tbody>
                                    @{
                                        for (int i = 0; i < contacts.Count; i++)
                                        {
                                            <tr>
                                                <td>@contacts[i].Title</td>
                                                <td>@contacts[i].Detail</td>
                                                <td>@contacts[i].Email</td>
                                                <td>@contacts[i].Name</td>
                                                <td>@contacts[i].DateCreated</td>
                                            </tr>
                                        }
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </main>
            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid px-4">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">Copyright &copy; Your Website 2021</div>
                        <div>
                            <a href="#">Privacy Policy</a>
                            &middot;
                            <a href="#">Terms &amp; Conditions</a>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="/profile/js/scripts.js"></script>

    <script src="/profile/js/dataTableGok.js"></script>
    <script src="/profile/js/datatables-simple-demo.js"></script>
</body>
</html>






















